; semi-colon is the comment character
[sortorder]
;===============================================================================
;
;   @(#)binary.srt	84.1
;
;   Sort Order Overview:
;   --------------------
;   Based on the ISO 8859/1 character set, this is a binary sort order.
;
;===============================================================================

class = 0x01			; Class `1' sort order
id = 0x32			; id = 50
name = bin_iso_1
menuname = "Binary ordering, for the ISO 8859/1 or Latin-1 character set (iso_1)."
charset = iso_1

description = "Binary sort order for the ISO 8859/1 character set (iso_1)."

binary = "true"
