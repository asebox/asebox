; semi-colon is the comment character
[sortorder]
;===============================================================================
;
;   Sort Order Overview:
;   --------------------
;   Based on the cp936 simplified Chinese character set, this sort 
;   order is a binary ordering.
;
;===============================================================================

class = 0x01			; Class `1' sort order
id = 0x32			; id = 50
name = bin_cp936
menuname = "Binary ordering, for cp936."
charset = cp936

description = "Binary sort order for simplified Chinese using cp936."

binary = "true"
