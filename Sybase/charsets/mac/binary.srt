; semi-colon is the comment character
[sortorder]
;===============================================================================
;
;   @(#)binary.srt	84.1
;
;   Sort Order Overview:
;   --------------------
;   Based on the Macintosh character set, this is a binary sort order.
;
;===============================================================================

class = 0x01			; Class `1' sort order
id = 0x32			; id = 50
name = bin_mac
menuname = "Binary ordering, for use with the Macintosh character set (mac)."
charset = mac

description = "Binary Sort Order for the Macintosh Character Set (mac)."

binary = "true"
