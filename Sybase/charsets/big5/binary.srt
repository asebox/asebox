; semi-colon is the comment character
[sortorder]
;===============================================================================
;
;   @(#) binary.srt 1.1	12/6/94
;
;   Sort Order Overview:
;   --------------------
;   Based on the BIG5 traditional Chinese character set, this sort 
;   order is a binary ordering.
;
;===============================================================================

class = 0x01			; Class `1' sort order
id = 0x32			; id = 50
name = bin_big5
menuname = "Binary ordering, for the BIG5 character set (big5)."
charset = big5

description = "Binary sort order for traditional Chinese using the BIG5 character set (big5)."

binary = "true"
