; semi-colon is the comment character
[sortorder]
;===============================================================================
;
;   %Z% %M% %I%	%G%
;
;   Sort Order Overview:
;   --------------------
;   Based on the EUC GB2312-80 simplified Chinese character set, this sort 
;   order is a binary ordering.
;
;===============================================================================

class = 0x01			; Class `1' sort order
id = 0x32			; id = 50
name = bin_eucgb
menuname = "Binary ordering, for the EUC GB2312-80 character set (eucgb)."
charset = eucgb

description = "Binary sort order for simplified Chinese using the EUC GB2312-80 character set (eucgb)."

binary = "true"
