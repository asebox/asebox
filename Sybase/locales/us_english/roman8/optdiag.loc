; LOC_SECT_INIT
;
;   Message strings related to initialization
;
[init]

1 = ZZZZZ, "Syntax error with quotation mark."
2 = ZZZZZ, "Column name was specified but table name was not."
3 = ZZZZZ, "Trace flag number exceeds %1!, the maximum allowable value."
5 = ZZZZZ, "Both input and output file names were specified."
6 = ZZZZZ, "The arguments for database and input file were not read, so the target\\ndatabase is unknown."
7 = ZZZZZ, "Failed to open the input file."
8 = ZZZZZ, "Failed to open the output file."
9 = ZZZZZ, "  Probably the file does not exist or Optdiag does not have\\nthe necessary permission."
10 = ZZZZZ, "Command line syntax error before the 'statistics' keyword was found."


; LOC_SECT_MESSAGES
;
;   Informational message strings
;
[messages]

1 = ZZZZZ, "Optdiag failed.  Check for error messages above."
2 = ZZZZZ, "Optdiag succeeded."
3 = ZZZZZ, "Set trace flag 1 on the command line (-T1) to ignore a difference\\nin Adaptive Server Enterprise versions."
5 = ZZZZZ, "Set trace flag 4 on the command line (-T4) to skip this\\nconsistency check."
7 = ZZZZZ, "not specified"
8 = ZZZZZ, " (clustered)"
9 = ZZZZZ, " (nonclustered)"
10 = ZZZZZ, "default used"
11 = ZZZZZ, "default"
12 = ZZZZZ, "Next table is '%1!' and the owner is '%2!'."
13 = ZZZZZ, "If the location of the problem is not clear, consider rerunning Optdiag\\nin input mode with trace flag 6 to help locate the problem."
14 = ZZZZZ, "Optdiag is done, but statistics were skipped.\\nCheck for error messages above."
15 = ZZZZZ, " (actual)"
16 = ZZZZZ, " (simulated)"
17 = ZZZZZ, "Statistics originated from upgrade of distribution page."
18 = ZZZZZ, "Statistics loaded from Optdiag."
19 = ZZZZZ, " (only 255 bytes used)"


