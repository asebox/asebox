;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; The default errors
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

[ctdrvdefaults]

1 = CS_DEF_MEMORY_ERRNUM, "impossible d'allouer de la mémoire."

2 = CS_DEF_LAYER_ERRNUM, "ctdrvlib"

3 = CS_DEF_ORIGIN_ERRNUM, "ctdrvlib"

4 = CS_DEF_ERROR_ERRNUM, "erreur générale"



;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; The CTDRV-Lib layers.
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

[ctdrvlayer]

1 = E_LAYER_BLK, "couche ctdrv"


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; The "origin of error" definitons.
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

[ctdrvorigin]

1 = E_ORG_USER, "erreur utilisateur"

2 = E_ORG_INTRN, "erreur interne sur CTDRV-Library "

3 = E_ORG_CT, "erreur sur bibliothèque CT"

4 = E_ORG_ASYNC, "erreur sur routine de gestion asynchrone"

5 = E_ORG_COM, "erreur sur bibliothèque commune"

6 = E_ORG_MEM, "erreur de gestion de mémoire"

7 = E_ORG_SS, "erreur sur SQL Server"

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; The User API Layer Errors
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

[ctdrvapierror]

0 = CTDRVE_OK, "ctdrv OK"

1 = CTDRVE_MEM_F, "Mémoire insuffisante pour la routine CtDrv."

2 = CTDRVE_CTF, "Echec de la routine CtDrv lors de l'appel de routine %1! de CT_Lib."

3 = CTDRVE_APIF, "Echec de la routine CtDrv lors de l'appel de %1!."

4 = CTDRVE_NOCURSOR, "Il n'y pas de curseur ouvert sur cette session."

5 = CTDRVE_TRUNCATE, "Toutes les données n'ont pas pu être renvoyées. %1! est tronqué."

6 = CTDRVE_DUPCUR, "Il existe déjà un curseur nommé %1!."

7 = CTDRVE_LENCUR, "Le nom du curseur ne doit pas dépasser %1! caractères."

8 = CTDRVE_SEQ, "La fonction est appelée hors séquence."

9 = CTDRVE_ODBCTYPE, "Impossible de mapper le type de données CT_LIB en entrée en type de données ODBC."

10 = CTDRVE_CTTYPE, "Impossible de mapper le type de données ODBC en entrée en type de données CT_LIB."

11 = CTDRVE_CTDATYPE, "%1! : impossible de trouver le type de données ct_lib."

12 = CTDRVE_ASYNC, "Fonction hors séquence : une autre fonction est en cours d'exécution asynchrone."

13 = CTDRVE_BUFF, "Echec de la routine CtDrv lors de l'allocation du tampon SQL."

14 = CTDRVE_SUBF, "Echec de la routine CtDrv lors de substitutions SQL."

15 = CTDRVE_STMTNM, "Echec de la routine CtDrv lors de l'élaboration du nom de l'instruction."

16 = CTDRVE_CURNM, "Echec de la routine CtDrv lors de l'élaboration du nom du curseur."

17 = CTDRVE_RESROW, "Echec de la routine CtDrv lors de l'obtention des résultats de ligne pour %1! et %2!."

18 = CTDRVE_RESDCF, "Echec de la routine CtDrv lors de l'obtention des résultats %1!."

19 = CTDRVE_APIF2, "Echec de la routine CtDrv lors de l'appel de la fonction interne %1!."

20 = CTDRVE_APIF3, "Echec de la fonction interne %1! lors de l'appel de routine CT_LIB %2!."

21 = CTDRVE_PARF, "Echec de la routine CtDrv lors de l'analyse SQL."

