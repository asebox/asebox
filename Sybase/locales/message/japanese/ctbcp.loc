3 = ZZZZZ, "フィールド %1! の長さを入力してください [上限なし]："
4 = ZZZZZ, "フィールド %1! の長さを入力してください [%2!]："
5 = ZZZZZ, "フィールド・ターミネータを入力してください [%1!]："
6 = ZZZZZ, "フィールド %1! のプレフィクス長を入力してください [%2!]："
7 = ZZZZZ, "フィールド %1! の精度を入力してください [%2!]："
8 = ZZZZZ, "フィールド %1! の位取りを入力してください [%2!]："

